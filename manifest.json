{
    "manifest_version": 3,
    "name": "Tabfinity",
    "version": "1.0",
    "description": "Organize and launch unlimited website shortcuts from your new tab page.",
    "chrome_url_overrides": {
      "newtab": "index.html"
    },
    "icons": {
      "128": "public/icon.png"
    },
    "permissions": ["storage"],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_title": "Tabfinity"
    }
  }
  